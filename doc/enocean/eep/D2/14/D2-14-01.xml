<eep>
  <profile>
    <rorg>
      <number>0xD2</number>
      <title>VLD Telegram</title>
      <telegram>VLD</telegram>
      <func>
        <number>0x14</number>
        <title>Multi Function Sensors</title>
        <type>
          <number>0x01</number>
          <title>Sensor for Temperature, Humidity, Illumination and Button A and B, line-powered</title>
          <status>released</status>
          <description>
            <span style="background-color:#CDFF7D;border-bottom:2px groove #000000;font-style:italic;">Submitter: Perfactory</span>
          </description>
          <case>
            <description>
              <img>graphics/D2-14-01.png</img>
            </description>
            <datafield>
              <reserved/>
              <bitoffs>21</bitoffs>
              <bitsize>3</bitsize>
            </datafield>
            <datafield>
              <data>Temperature</data>
              <shortcut>TMP9</shortcut>
              <description>Temperature (linear)<br/>-----------
                  <br/>Status of Temperature Sensor
                </description>
              <info/>
              <bitoffs>0</bitoffs>
              <bitsize>9</bitsize>
              <enum>
                <item>
                  <min>0</min>
                  <max>500</max>
                  <scale>
                    <min>0</min>
                    <max>50</max>
                  </scale>
                  <unit>&#176;C</unit>
                </item>
                <item>
                  <min>501</min>
                  <max>510</max>
                  <description>Reserved</description>
                </item>
                <item>
                  <value>511</value>
                  <description>Error</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Humidity</data>
              <shortcut>HUM</shortcut>
              <description>Rel. Humidity (linear)<br/>-----------
                  <br/>Status of Humidity Sensor
                </description>
              <info/>
              <bitoffs>9</bitoffs>
              <bitsize>8</bitsize>
              <enum>
                <item>
                  <min>0</min>
                  <max>200</max>
                  <scale>
                    <min>0</min>
                    <max>100</max>
                  </scale>
                  <unit>%</unit>
                </item>
                <item>
                  <min>201</min>
                  <max>254</max>
                  <description>Reserved</description>
                </item>
                <item>
                  <value>255</value>
                  <description>Error</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Button A</data>
              <shortcut>BA</shortcut>
              <description>Button A</description>
              <info/>
              <bitoffs>17</bitoffs>
              <bitsize>2</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Button A released</description>
                </item>
              </enum>
              <enum>
                <item>
                  <value>1</value>
                  <description>Button A pressed</description>
                </item>
              </enum>
              <enum>
                <item>
                  <value>2</value>
                  <description>Reserved</description>
                </item>
              </enum>
              <enum>
                <item>
                  <value>3</value>
                  <description>Button A error (state not detectable)</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Button B</data>
              <shortcut>BB</shortcut>
              <description>Button B</description>
              <info/>
              <bitoffs>19</bitoffs>
              <bitsize>2</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Button B released</description>
                </item>
              </enum>
              <enum>
                <item>
                  <value>1</value>
                  <description>Button B pressed</description>
                </item>
              </enum>
              <enum>
                <item>
                  <value>2</value>
                  <description>Reserved</description>
                </item>
              </enum>
              <enum>
                <item>
                  <value>3</value>
                  <description>Button B error (state not detectable)</description>
                </item>
              </enum>
            </datafield>
