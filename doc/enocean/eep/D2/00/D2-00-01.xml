                  <description>Presence</description>
                </item>
                <item>
                  <value>0x02</value>
                  <description>Temperature Set Point &#8220;down&#8221; or &#8220;&#8212;&#8220;</description>
                </item>
                <item>
                  <value>0x03</value>
                  <description>not used</description>
                </item>
                <item>
                  <value>0x04</value>
                  <description>not used</description>
                </item>
                <item>
                  <value>0x05</value>
                  <description>Temperature Set Point &#8220;up&#8221; or &#8220;+&#8221;</description>
                </item>
                <item>
                  <value>0x06</value>
                  <description>Fan</description>
                </item>
                <item>
                  <min>0x07</min>
                  <max>0x1F</max>
                  <description>Not Used</description>
                </item>
              </enum>
            </datafield>
