                </item>
                <item>
                  <min>0x1</min>
                  <max>0x7</max>
                  <description>1 &#8230; 7</description>
                  <scale>
                    <min>1</min>
                    <max>7</max>
                  </scale>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Keep Alive Timing</data>
