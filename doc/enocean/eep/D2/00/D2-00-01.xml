            </datafield>
            <datafield>
              <data>Set PointSteps</data>
              <shortcut>SPS</shortcut>
              <description>Number of Set Point Steps:<br/><br/>
                REMARK:<br/>Specifies the number of equidistant steps between 0 and Set Point Range Limit
                </description>
              <info/>
              <bitoffs>17</bitoffs>
              <bitsize>7</bitsize>
              <enum>
                <item>
                  <value>0x00</value>
                  <description>Set Point disabled</description>
                </item>
                <item>
                  <min>0x01</min>
                  <max>0x7F</max>
                  <description>1 &#8230; 127</description>
                  <scale>
                    <min>1</min>
                    <max>127</max>
                  </scale>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Temperature Measurement Timing</data>
              <shortcut>TT (LSB)</shortcut>
              <description>Time between two subsequent Temperature measurements<br/><br/>
                  LSB (Bit 3 ... 0)
                </description>
              <info/>
              <bitoffs>24</bitoffs>
              <bitsize>4</bitsize>
              <enum>
                <item>
                  <value>0x00</value>
                  <description>Temperature measurement disabled</description>
                </item>
                <item>
                  <min>0x01</min>
                  <max>0x3C</max>
                  <description>10 &#8230; 600s [10s]</description>
                  <scale>
                    <min>10</min>
                    <max>600</max>
                  </scale>
                  <unit>s</unit>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Temperature Measurement Timing</data>
              <shortcut>TT (MSB)</shortcut>
              <description>Time between two subsequent Temperature measurements<br/><br/>
                  MSB (Bit 5 ... 4)
                </description>
              <info/>
              <bitoffs>38</bitoffs>
              <bitsize>2</bitsize>
            </datafield>
            <datafield>
              <data>Fan</data>
              <shortcut>F</shortcut>
              <description>Number of Fan Speed Levels available to user:</description>
              <info/>
              <bitoffs>35</bitoffs>
              <bitsize>3</bitsize>
              <enum>
                <item>
                  <value>0x0</value>
                  <description>Fan Speed disabled</description>
                </item>
                <item>
                  <min>0x1</min>
                  <max>0x7</max>
                  <description>1 &#8230; 7</description>
                  <scale>
                    <min>1</min>
                    <max>7</max>
                  </scale>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Presence</data>
              <shortcut>PR</shortcut>
              <description>Number of Presence Levels available to user</description>
              <info/>
              <bitoffs>32</bitoffs>
              <bitsize>3</bitsize>
              <enum>
                <item>
                  <value>0x0</value>
