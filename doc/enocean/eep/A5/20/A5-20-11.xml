              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Not disabled</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Disabled</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Key card disablement</data>
              <shortcut>KCD</shortcut>
              <description>Key carddisablement</description>
              <info/>
              <bitoffs>22</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Not disabled</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Disabled</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>External disablement</data>
              <shortcut>ED</shortcut>
              <description>External disablement</description>
              <info/>
              <bitoffs>23</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Not disabled</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Disabled</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Window contact</data>
              <shortcut>WC</shortcut>
              <description>Window contact</description>
              <info/>
              <bitoffs>30</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Windows opened</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Windows closed</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Alarm State</data>
              <shortcut>AS</shortcut>
              <description>Alarm State</description>
              <info/>
              <bitoffs>31</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Ok</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Error</description>
                </item>
              </enum>
            </datafield>
          </case>
        </type>
      </func>
    </rorg>
  </profile>
</eep>
