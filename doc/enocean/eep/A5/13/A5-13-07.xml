<eep>
  <profile>
    <rorg>
      <number>0xA5</number>
      <title>4BS Telegram</title>
      <telegram>4BS</telegram>
      <func>
        <number>0x13</number>
        <title>Environmental Applications</title>
        <type>
          <number>0x07</number>
          <title>Wind Sensor</title>
          <status>released</status>
          <description><span style="background-color:#CDFF7D;border-bottom:2px groove #000000;font-style:italic;">Submitter: Hideki Electronics Limited</span><br/><br/><span style="border-bottom:2px groove #000000;">Description</span><br/>This profile provides wind sensor information.
            <br/>That includes the current wind direction, the average and the
            maximum wind speed.
            <br/><br/><span style="border-bottom:2px groove #000000;">Data exchange</span><br/>Direction: unidirectional
            <br/>Addressing: broadcast
            <br/>Communication trigger: time-triggered
            <br/>Communication interval: 33 seconds
            <br/>Trigger event: timer
            <br/>Tx delay: N/A
            <br/>Rx timeout: N/A
            <br/><br/><span style="border-bottom:2px groove #000000;">Teach-in</span><br/>Teach-in method: 4BS teach-in 1
            <br/><br/><span style="border-bottom:2px groove #000000;">Security</span><br/>Encryption supported: no
            <br/>Security level format: N/A
          </description>
          <case>
            <datafield>
              <reserved/>
              <bitoffs>0</bitoffs>
              <bitsize>4</bitsize>
            </datafield>
            <datafield>
              <reserved/>
              <bitoffs>24</bitoffs>
              <bitsize>4</bitsize>
            </datafield>
            <datafield>
              <reserved/>
              <bitoffs>29</bitoffs>
              <bitsize>2</bitsize>
            </datafield>
            <datafield>
              <data>LRN Bit</data>
              <shortcut>LRNB</shortcut>
              <description>LRN Bit</description>
              <bitoffs>28</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Teach-in telegram</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Data telegram</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Wind Direction</data>
              <shortcut>WD</shortcut>
              <description>Shows the current wind direction</description>
              <bitoffs>4</bitoffs>
              <bitsize>4</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>NNE</description>
                </item>
                <item>
                  <value>1</value>
                  <description>NE</description>
                </item>
                <item>
                  <value>2</value>
                  <description>ENE</description>
                </item>
                <item>
                  <value>3</value>
                  <description>E</description>
                </item>
                <item>
                  <value>4</value>
                  <description>ESE</description>
                </item>
                <item>
                  <value>5</value>
                  <description>SE</description>
                </item>
                <item>
                  <value>6</value>
                  <description>SSE</description>
                </item>
                <item>
                  <value>7</value>
                  <description>S</description>
                </item>
                <item>
                  <value>8</value>
                  <description>SSW</description>
                </item>
                <item>
                  <value>9</value>
                  <description>SW</description>
                </item>
                <item>
                  <value>10</value>
                  <description>WSW</description>
                </item>
                <item>
                  <value>11</value>
                  <description>W</description>
                </item>
                <item>
                  <value>12</value>
                  <description>WNW</description>
                </item>
                <item>
                  <value>13</value>
                  <description>NW</description>
                </item>
                <item>
                  <value>14</value>
                  <description>NNW</description>
                </item>
                <item>
                  <value>15</value>
                  <description>N</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Average Wind Speed</data>
              <shortcut>AWS</shortcut>
              <description>Linear average wind speed</description>
              <info/>
              <bitoffs>8</bitoffs>
              <bitsize>8</bitsize>
              <range>
                <min>0</min>
                <max>255</max>
              </range>
              <scale>
                <min>1</min>
                <max>199.9</max>
              </scale>
              <unit>mph</unit>
            </datafield>
            <datafield>
              <data>Maximum Wind Speed</data>
              <shortcut>MWS</shortcut>
              <description>Maximum wind speed</description>
              <info/>
              <bitoffs>16</bitoffs>
              <bitsize>8</bitsize>
              <range>
                <min>0</min>
                <max>255</max>
              </range>
              <scale>
                <min>1</min>
                <max>199.9</max>
              </scale>
              <unit>mph</unit>
            </datafield>
            <datafield>
              <data>Battery Status</data>
              <shortcut>BS</shortcut>
              <description>Indicates if the battery is low</description>
              <bitoffs>31</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Battery okay</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Battery low</description>
                </item>
              </enum>
            </datafield>
          </case>
        </type>
      </func>
    </rorg>
  </profile>
</eep>
