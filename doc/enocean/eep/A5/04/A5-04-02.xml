          <case>
            <datafield>
              <reserved/>
              <bitoffs>0</bitoffs>
              <bitsize>8</bitsize>
            </datafield>
            <datafield>
              <reserved/>
              <bitoffs>24</bitoffs>
              <bitsize>4</bitsize>
            </datafield>
            <datafield>
              <reserved/>
