<eep>
  <profile>
    <rorg>
      <number>0xA5</number>
      <title>4BS Telegram</title>
      <telegram>4BS</telegram>
      <func>
        <number>0x10</number>
        <title>Room Operating Panel</title>
        <type>
          <number>0x1F</number>
          <title>Temperature Sensor, Set Point, Fan Speed, Occupancy and Unoccupancy Control</title>
          <description>
            <span style="background-color:#CDFF7D;border-bottom:2px groove #000000;font-style:italic;">Submitter: Distech Controls</span>
          </description>
          <case>
            <datafield>
              <data>Turn-switch for fan speed</data>
              <shortcut>FAN</shortcut>
              <description>Turn-switch for fan speed</description>
              <info/>
              <bitoffs>0</bitoffs>
              <bitsize>8</bitsize>
              <enum>
                <item>
                  <min>210</min>
                  <max>255</max>
                  <description>Stage auto</description>
                </item>
                <item>
                  <min>190</min>
                  <max>209</max>
                  <description>Stage 0</description>
                </item>
                <item>
                  <min>165</min>
                  <max>189</max>
                  <description>Stage 1</description>
                </item>
                <item>
                  <min>145</min>
                  <max>164</max>
                  <description>Stage 2</description>
                </item>
                <item>
                  <min>0</min>
                  <max>144</max>
                  <description>Stage 3</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Set Point</data>
              <shortcut>SP</shortcut>
              <description>Set point (linear) Min.- ... Max+</description>
              <info/>
              <bitoffs>8</bitoffs>
              <bitsize>8</bitsize>
              <range>
                <min>0</min>
                <max>255</max>
              </range>
              <scale>
                <min>0</min>
                <max>255</max>
              </scale>
              <unit>N/A</unit>
            </datafield>
            <datafield>
              <data>Temperature</data>
              <shortcut>TMP</shortcut>
              <description>Temperature (linear)</description>
              <info/>
              <bitoffs>16</bitoffs>
              <bitsize>8</bitsize>
              <range>
                <min>255</min>
                <max>0</max>
              </range>
              <scale>
                <min>0</min>
                <max>+40</max>
              </scale>
              <unit>&#176;C</unit>
            </datafield>
            <datafield>
              <reserved/>
              <bitoffs>24</bitoffs>
              <bitsize>1</bitsize>
            </datafield>
            <datafield>
              <data>Temperature flag</data>
              <shortcut>TMP_F</shortcut>
              <description>Temperature flag</description>
              <bitoffs>25</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>1</value>
                  <description>Temperature present</description>
                </item>
                <item>
                  <value>0</value>
                  <description>Temperature absent</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Set point flag</data>
              <shortcut>SP_F</shortcut>
              <description>Set point flag</description>
              <bitoffs>26</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>1</value>
                  <description>Set point present</description>
                </item>
                <item>
                  <value>0</value>
                  <description>Set point absent</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Fan speed flag</data>
              <shortcut>FAN_F</shortcut>
              <description>Fan speed flag</description>
              <bitoffs>27</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>1</value>
                  <description>Fan speed present</description>
                </item>
                <item>
                  <value>0</value>
                  <description>Fan speed absent</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <reserved/>
              <bitoffs>29</bitoffs>
              <bitsize>1</bitsize>
            </datafield>
            <datafield>
              <data>LRN Bit</data>
              <shortcut>LRNB</shortcut>
              <description>LRN Bit</description>
              <bitoffs>28</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Teach-in telegram</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Data telegram</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Unoccupancy</data>
              <shortcut>UNOCC</shortcut>
              <description>Unoccupancy button</description>
              <info/>
              <bitoffs>30</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Button pressed</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Button released</description>
                </item>
              </enum>
            </datafield>
            <datafield>
              <data>Occupancy</data>
              <shortcut>OCC</shortcut>
              <description>Occupancy button</description>
              <info/>
              <bitoffs>31</bitoffs>
              <bitsize>1</bitsize>
              <enum>
                <item>
                  <value>0</value>
                  <description>Button pressed</description>
                </item>
                <item>
                  <value>1</value>
                  <description>Button released</description>
                </item>
              </enum>
            </datafield>
          </case>
        </type>
      </func>
    </rorg>
  </profile>
</eep>
